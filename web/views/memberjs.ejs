<% if (!user) { %>
  window.location = '/login';
<% } else { %>
  var senseBase = {
    user : '<%= user.username %>',
  <% if (logo) { %>
    logo : '<%= logo %>',
    homepage : '<%= homepage %>',
  <% } %>
  <% if (collab) { %>
    collab : true,
  <% } %>
    isScraper : <%= user.type == 'Scraper' %>
  }
<% } %>


